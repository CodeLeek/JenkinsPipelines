pipeline {
    agent any

    parameters{
        choice(choices: ["TEST", "DEV", "QA", 'PRE-PROD', 'PROD'], description: "Which ENV to deploy in?", name: 'deployEnv')
    }

    stages{
        stage("Demo"){
            steps {
                echo "Choice is set to: ${params.deployEnv}"

            }
        }
    }
}